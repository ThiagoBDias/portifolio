{
  "version": 2,
  "name": "portifolio",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ],
  "buildCommand": "cd site && npm run build",
  "outputDirectory": "site/dist",
  "installCommand": "cd site && npm ci",
  "framework": "astro",
  "regions": ["gru1"]
}
