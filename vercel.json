{
  "version": 2,
  "buildCommand": "cd site && npm run build",
  "outputDirectory": "site/dist",
  "installCommand": "cd site && npm install",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ],
  "regions": ["gru1"],
  "functions": {
    "site/src/pages/**/*.astro": {
      "runtime": "nodejs18.x"
    }
  }
}
